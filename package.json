{
	"name": "gamerbits-api",
	"version": "0.5.0",
	"description": "A simple Game Review Aggregator API",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"dev": "cross-env NODE_ENV=development tsx watch src/server.ts",
		"dev:pull": "npm run dev:load-envs prisma db pull",
		"dev:generate": "npm run dev:load-envs prisma generate",
		"dev:migrate": "npm run dev:load-envs prisma migrate dev --name init",
		"dev:seed": "npm run dev:load-envs prisma db seed",
		"dev:load-envs": "dotenv -e .env.development",
		"test": "cross-env NODE_ENV=test jest --passWithNoTests --runInBand",
		"test:load-envs": "dotenv -e .env.test",
		"test:seed": "npm run test:load-envs prisma db seed",
		"test:migration:run": "npm run test:load-envs prisma migrate deploy",
		"test:migration:generate": "npm run test:load-envs prisma migrate dev",
		"test:watch": "cross-env NODE_ENV=test jest --watch --passWithNoTests --runInBand"
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@prisma/client": "^4.12.0",
		"bcrypt": "^5.1.0",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"dotenv-expand": "^10.0.0",
		"express": "^4.18.2",
		"express-async-errors": "^3.1.1",
		"joi": "^17.9.1",
		"jsonwebtoken": "^9.0.0"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/jest": "^29.5.1",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/node": "^18.15.11",
		"@types/supertest": "^2.0.12",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^7.2.1",
		"faker": "^6.6.6",
		"jest": "^29.5.0",
		"prisma": "^4.12.0",
		"supertest": "^6.3.3",
		"ts-node": "^10.9.1",
		"tsx": "^3.12.6",
		"typescript": "^5.0.4",
		"typescript-transform-paths": "^3.4.6"
	}
}
